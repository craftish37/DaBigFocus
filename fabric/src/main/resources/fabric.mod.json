{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "https://github.com/craftish37/DaBigFocus"
  },
  "license": "${license}",
  "icon": "logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.craftish37.dabigfocus.fabric.DaBigFocusFabric"
    ],
    "modmenu" : [
      "net.craftish37.dabigfocus.fabric.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  },
  "suggests": {
    "sodium": ">=0.5.9"
  }
}
  